<% if @success == true %>
    $("#contact_message").html("<div class='alert alert-success' role='alert'>Thank you for contacting us!</div>");
<% else %>
    $("#contact_message").html("<div class='alert alert-danger' role='alert'>Oops, something went wrong.<% @errors.full_messages.each do |error| %><li><%= error %></li><% end %></div>");
<% end %>
